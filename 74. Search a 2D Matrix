class Solution {
public:
    bool searchMatrix(vector<vector<int>>& nums, int target) {
    unordered_map<int, int>mp;
    int key=0;
    for(int i =0;i < nums.size(); i++){
        for(int j=0;j<nums[0].size();j++){
            mp.insert({nums[i][j],key});key++;
            }
    }

    if(mp.find(target) != mp.end())return true;
    return false;
    }
};
